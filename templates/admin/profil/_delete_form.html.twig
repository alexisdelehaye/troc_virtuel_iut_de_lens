<form method="post" action="{{ path('admin_profil_delete', {'idprofil': profil.idprofil}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer le profil ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ profil.idprofil) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>

