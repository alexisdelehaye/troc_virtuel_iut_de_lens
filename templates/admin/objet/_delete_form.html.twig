<form method="post" action="{{ path('admin_objet_delete', {'idobjet': objet.idobjet}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objet.idobjet) }}">
    <button class="btn btn-danger">Delete</button>
</form>
