<form method="post" action="{{ path('profil_delete', {'idprofil': profil.idprofil}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ profil.idprofil) }}">
    <button class="btn">Delete</button>
</form>
